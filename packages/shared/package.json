{
  "name": "@monorepo/shared",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cd ../../apps/frontend && pnpm run dev",
    "build": "cd ../../apps/frontend && pnpm run build",
    "preview": "cd ../../apps/frontend && pnpm run preview",
    "dev:old": "cd ../../front-old && pnpm run dev",
    "backend": "cd ../../ && ./start_backend.sh",
    "lint": "eslint ../../apps/frontend/src ../../packages/ui/src --ext .ts,.vue",
    "lint:fix": "eslint ../../apps/frontend/src ../../packages/ui/src --ext .ts,.tsx,.vue --fix",
    "format": "prettier --check ../../apps/frontend/src ../../packages/ui/src",
    "format:fix": "prettier --write ../../apps/frontend/src ../../packages/ui/src",
    "type-check": "vue-tsc --noEmit --project ../../apps/frontend/tsconfig.app.json && vue-tsc --noEmit --project ../../packages/ui/tsconfig.json",
    "type-check:watch": "vue-tsc --noEmit --watch --project ../../apps/frontend/tsconfig.app.json"
  },
  "devDependencies": {
    "@monorepo/typescript-config": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.2.4",
    "vue-eslint-parser": "^9.0.0",
    "vue-tsc": "^2.2.12"
  }
}
